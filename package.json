  "description": "Шаблон для курса «Основы веб разработки»",
  "type": "module",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "check:js-style-rules": "eslint .",
    "check:css-style-rules": "stylelint */*.css",
    "check:format": "prettier . --check",
    "check:a11y": "node check-a11y.js",
    "fix:js-style-rules": "eslint . --fix",
    "fix:css-style-rules": "stylelint */*.css --fix",
    "fix:format": "prettier . --write",
    "create:commit": "npx better-commits"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtkishkinVlad/kontur-web-course-template.git"
  },
