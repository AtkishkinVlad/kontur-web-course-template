    "template"
  ],
  "author": "atkvs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtkishkinVlad/kontur-web-course-template/issues"
  },
  "homepage": "https://github.com/AtkishkinVlad/kontur-web-course-template#readme",
  "devDependencies": {
    "@eslint/js": "9.18.0",
    "axe-core": "4.10.2",
    "axe-puppeteer": "1.1.1",
    "eslint": "9.18.0",
    "globals": "15.14.0",
    "http-server": "14.1.1",
    "prettier": "3.4.2",
    "puppeteer": "24.1.0",
    "stylelint": "16.13.2",
    "stylelint-config-standard": "37.0.0"
  }
